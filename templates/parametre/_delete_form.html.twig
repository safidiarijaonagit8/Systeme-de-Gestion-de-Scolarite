<form method="post" action="{{ path('app_parametre_delete', {'id': parametre.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ parametre.id) }}">
    <div style="text-align: center;">
    <button class="btn btn-danger" >Supprimer</button>
    </div>
</form>
